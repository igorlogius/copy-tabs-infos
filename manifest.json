{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",

  "browser_action": {
    "browser_style": true,
    "default_area": "navbar",
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "dark-256.png",
        "dark": "icon.png",
        "size": 256
      }
    ]
  },
  "icons": {
    "256": "icon.png"
  },
  "manifest_version": 2,
  "name": "Get Tabs Infos",
  "description": "Get tab information in a user defined custom format string",
  "permissions": ["tabs", "storage"],
  "version": "1.10.11"
}
